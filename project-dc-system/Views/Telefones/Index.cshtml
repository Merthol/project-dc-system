@model IEnumerable<project_dc_system.Models.Telefone>

@{
    ViewData["Title"] = "Index";
}

<h1>Telefones</h1>

<p>
    <a asp-action="Create">Incluir novo telefone</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cliente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente.ClienteName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TelefoneId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.TelefoneId">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.TelefoneId">Excluir</a>
            </td>
        </tr>
}
    </tbody>
</table>
